      RowBox[{"end", "-", "start"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"dir", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"pos", "=", "start"}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "\[LessEqual]", "bisections"}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"pos", "=", 
         RowBox[{"pos", "+", 
          RowBox[{"dir", " ", 
