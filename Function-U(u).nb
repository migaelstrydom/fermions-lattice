        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"sign2", "\[NotEqual]", "sign1"}], ",", 
          RowBox[{"dir", "=", 
           RowBox[{"-", "dir"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"sign1", "=", "sign2"}], ";"}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", 
