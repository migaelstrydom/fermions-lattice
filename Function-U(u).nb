   "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "sign1", ",", "interval", ",", "dir", ",", "pos", ",", "sign2", ",", 
      "i"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"sign1", "=", 
      RowBox[{"Sign", "[", 
       RowBox[{"func", "[", "start", "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
